<Window x:Class="UserWallPaper.MultiProgressBar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MultiProgressbar" Height="110" Width="425" WindowStyle="None" ResizeMode="NoResize" AllowsTransparency="True" Background="Transparent"
        WindowStartupLocation="CenterScreen" ShowInTaskbar="False" Topmost="True">
    <Grid>
        <ProgressBar x:Name="pb" HorizontalAlignment="Left" Height="25" Margin="120,70,0,0" VerticalAlignment="Top" Width="282"/>
        <ProgressBar x:Name="total" HorizontalAlignment="Left" Height="25" Margin="120,35,0,0" VerticalAlignment="Top" Width="282"/>
        <Label Content="전체 진행" HorizontalAlignment="Left" Height="28" Margin="15,35,0,0" VerticalAlignment="Top" Width="70"/>
        <Label Content="현제 파일 진행" HorizontalAlignment="Left" Height="28" Margin="15,70,0,0" VerticalAlignment="Top" Width="95"/>
        <Label HorizontalAlignment="Left" Content="" VerticalAlignment="Top" Margin="15,5,0,0" x:Name="Title" />
        <TextBlock x:Name="txtFile" Margin="199,122,210,81" Text="Copying 0 of 0 files" TextWrapping="Wrap" HorizontalAlignment="Center" Visibility="Hidden"/>
    </Grid>
</Window>
